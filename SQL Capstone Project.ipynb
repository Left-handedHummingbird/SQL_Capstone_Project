    "N_Medals2[\"Income\"] = Medals2[\"Income\"]/Medals2[\"Income\"].max()\n",
    "N_Medals2[\"Medals\"] = Medals2[\"Medals\"]/Medals2[\"Medals\"].max()\n",
    "model=np.polyfit(N_Medals2[\"Income\"],N_Medals2[\"Medals\"],1)\n",
    "N_Medals2[\"y_hat\"]=N_Medals2[\"Income\"]*np.poly1d(model)[1]+np.poly1d(model)[0]\n",
    "Se=math.sqrt(((N_Medals2[\"Medals\"]-N_Medals2[\"y_hat\"])**2).sum()/(N_Medals2[\"Medals\"].count()-2))\n",
    "Sb=Se/math.sqrt((N_Medals2[\"Income\"]**2).sum()-N_Medals2[\"Income\"].count()*N_Medals2[\"Income\"].mean()**2)\n",
    "t = np.poly1d(model)[1]/Sb\n",
    "df = N_Medals2[\"Income\"].count()-2\n",
    "stats.t(t, df).args"
