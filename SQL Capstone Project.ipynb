   "metadata": {},
   "outputs": [],
   "source": [
    "N_Medals=pd.DataFrame()\n",
    "N_Medals[\"Population\"] = Medals[\"Population\"]/Medals[\"Population\"].max()\n",
    "N_Medals[\"Income\"] = Medals[\"Income\"]/Medals[\"Income\"].max()\n",
    "model=np.polyfit(N_Medals[\"Population\"],N_Medals[\"Income\"],1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "N_Medals[\"y_hat\"]=N_Medals[\"Population\"]*np.poly1d(model)[1]+np.poly1d(model)[0]\n",
    "Se=math.sqrt(((N_Medals[\"Income\"]-N_Medals[\"y_hat\"])**2).sum()/(N_Medals[\"Income\"].count()-2))\n",
    "Sb=Se/math.sqrt((N_Medals[\"Population\"]**2).sum()-N_Medals[\"Population\"].count()*N_Medals[\"Population\"].mean()**2)"
   ]
  },
  {
